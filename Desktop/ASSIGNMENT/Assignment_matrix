import java.util.*;
class Main
 {
	public static void main(String[] args) {
	
		int a[][];
		a=new int[2][3];
		Scanner d= new Scanner(System.in);
		for(int i=0;i<2;i++)
		{
			for(int j=0;j<3;j++)
			{
				int x;
				x=d.nextInt();
				a[i][j]=x;
			}
		}
		for(int i=0;i<2;i++)
		{
			for(int j=0;j<3;j++)
			{
				System.out.print(a[i][j] + " ");
			}
			System.out.println();
		}
	}
}
